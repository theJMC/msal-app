<script>
    document.addEventListener("DOMContentLoaded", async () => {
    const SB_client = supabase.createClient(SUPABASE_URL, SUPABASE_KEY)
    const { error } = await SB_client.auth.signOut()
    if (!error) {
        alert("Logout successful")
        window.location.href = "/"
    }
    console.log(error)
});
</script>